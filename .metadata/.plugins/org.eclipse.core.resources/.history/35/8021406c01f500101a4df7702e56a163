package exam;

public class ForExam1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// Q2739
		int n = 2;

		for (int i = 1; i < 10; i++) {
			System.out.println(n + "*" + i + "=" + (n * i));
		}

		System.out.println();

		// Q8393
		n = 3;
		int sum = 0;

		for (int i = 1; i < 3; i++) {
			sum = n + i;
		}
		System.out.println("총 합계 : " + sum);

	}

}
