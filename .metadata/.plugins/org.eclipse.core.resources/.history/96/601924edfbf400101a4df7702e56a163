package exam;

import java.util.Scanner;

public class WhileExam1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// Q2739
		Scanner multiply = new Scanner(System.in); // 숫자 입력 받음.
		System.out.print("숫자 입력 : ");

		int n = 1;
		while (n < 10) {
			int i = 1;
			while (i < 10) {
				System.out.println(n + "*" + i + "=" + (n * i));
				i++;
			}
			n++;
		}

		// Q8393
		int num = 3;
		int sum = 1;

		while (num > 1) {
			// 반복문 내에서는 변수를 만들지 않는다.
//			sum = multiply.nextInt();
			sum += num;
			num--;
		}
		System.out.println(sum);

		// Q25314
		int n = 20;
		String l = "";

		while (n >= 4) {
			l += "long ";
			n -= 4; // 4바이트이기 때문
		}
		l += "int";
		System.out.println(l);

		// Q2438
		n = 5;

		while (n < 10) {
			int i = 1;
			while (i < 10) {
				System.out.println(n + "*" + i + "=" + (n * i));
				i++;
			}
			n++;
		}

	}

}
