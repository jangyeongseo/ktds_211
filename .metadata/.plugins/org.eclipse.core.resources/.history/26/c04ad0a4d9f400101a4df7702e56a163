package exam;

public class IfExam3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// Q1330
		int A = 1;
		int B = 2;

		if (A > B) {
			System.out.println(">");
		} else if (A < B) {
			System.out.println("<");
		} else {
			System.out.println("==");
		}

		System.out.println();

		// Q9498
		int a = 100;

		if (a >= 90 && a <= 100) {
			System.out.println("A");
		} else if (a >= 80 && a <= 89) {
			System.out.println("B");
		} else if (a >= 70 && a <= 79) {
			System.out.println("C");
		} else if (a >= 60 && a <= 69) {
			System.out.println("D");
		} else {
			System.out.println("F");
		}

		System.out.println();

		// Q2753
		int year = 2000;

		if ((year % 4 == 0 && year % 100 != 0) || year % 400 == 0) {
			// 윤년은 연도가 4의 배수이면서, 100의 배수가 아닐 때 또는 400의 배수일 때이다.
			System.out.println("윤년 1");
		} else {
			System.out.println("윤년이 아니다 0");
		}

		System.out.println();

		// Q14681
		int x = 12;
		int y = -5;

		if (x == 12 && y == 5) {
			System.out.println("Quadrant 1");
		} else if (x == -12 && y == 5) {
			System.out.println("Quadrant 2");
		} else if (x == -12 && y == -5) {
			System.out.println("Quadrant 3");
		} else {
			System.out.println("Quadrant 4");
		}

		System.out.println();

		// Q2884
		int H = 10;
		int M = 10;
		
		int m = 10 * 60 + 60;
		m -= 45;
		
		H = m/60;
		M = m%60;
		
		System.out.println("시간" + H + "분" + M);
		
		
		

		// Q2525 - 오븐구이가 끝나는 시각을 계산하는 프로그램
		A = 14;
		B = 30;
		int c = 20;

		System.out.println("끝나는 시각 : " + A + "시" + B + "분");

		// Q2480
		int n = 3;
		int t = 3;
		int th = 6;
		
		int money = 10_000;
		int q = 1000;
		int p = 100;
		
		int same = 0;
		
		if(n == t) {
			same ++;
		}if (n )
		
		
	}

}
