package exam;

public class IfExam3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// Q1330
		int A = 1;
		int B = 2;

		if (A > B) {
			System.out.println(">");
		} else if (A < B) {
			System.out.println("<");
		} else {
			System.out.println("==");
		}

		System.out.println();

		// Q9498
		int a = 100;

		if (a >= 90 && a <= 100) {
			System.out.println("A");
		} else if (a >= 80 && a <= 89) {
			System.out.println("B");
		} else if (a >= 70 && a <= 79) {
			System.out.println("C");
		} else if (a >= 60 && a <= 69) {
			System.out.println("D");
		} else {
			System.out.println("F");
		}

		System.out.println();

		// Q2753
		int year = 2000;

		if ((year % 4 == 0 && year % 100 != 0) || year % 400 == 0) {
			// 윤년은 연도가 4의 배수이면서, 100의 배수가 아닐 때 또는 400의 배수일 때이다.
			System.out.println("윤년 1");
		} else {
			System.out.println("윤년이 아니다 0");
		}

		System.out.println();

		// Q14681
		int x = 12;
		int y = -5;

		if (x == 12 && y == 5) {
			System.out.println("Quadrant 1");
		} else if (x == -12 && y == 5) {
			System.out.println("Quadrant 2");
		} else if (x == -12 && y == -5) {
			System.out.println("Quadrant 3");
		} else {
			System.out.println("Quadrant 4");
		}

		System.out.println();

		// Q2884
		int H = 10;
		int M = 10;

		int totalMinutes = H * 60 + M;
		totalMinutes -= 45;
		// 45분 일짝 알람 설정하기 여서
		// 알람 = 24시간 * 60 -> 1440분이 나옴

		if (totalMinutes < 0) {
			totalMinutes += 1440;
		}

		H = totalMinutes / 60;
		M = totalMinutes % 60;

		System.out.println(H + "시간" + M + "분");

		// Case 2
		H = 10;
		M = 10;
		int earlyMiuntes = 45;

		int minutes = H * 60 + M;

		if (minutes < earlyMiuntes) {
			earlyMiuntes -= minutes;
			minutes = 24 * 60;
		}

		minutes -= earlyMiuntes;

		int alarmH = minutes / 60;
		int alarmM = minutes % 60;

		System.out.println(alarmH + "시간" + alarmM + "분");

		System.out.println();

		// Q2525 - 오븐구이가 끝나는 시각을 계산하는 프로그램
		A = 14;
		B = 30;
		int C = 20;

		totalMinutes = A * 60 + (B + C);

		if (totalMinutes < 0) {
			totalMinutes += 1440;
		}

		A = totalMinutes / 60;
		B = totalMinutes % 60;

		System.out.println("끝나는 시각 : " + A + "시" + B + "분");

		// Case 2
		A = 14;
		B = 30;
		int cooktime = 20;

		if (cooktime > 1440) {
			int days = cooktime / 1440;
			cooktime -= 1440 * days;
		}

		minutes = A * 60 + B;
		minutes += cooktime;
		if (minutes > 24 * 60) { // 요리가 끝난 시간이 하루를 넘어가면.
			minutes -= 24 * 60; // 요리가 끝난 시간에서 하루를 뺀
		}

		int alarmA = minutes / 60;
		int alarmB = minutes % 60;
		System.out.println("끝나는 시각 : " + alarmA + "시" + alarmB + "분");

		System.out.println();

		// Q2480
		int n = 3;
		int t = 3;
		int th = 6;

		int money;

		// 3개 모두 같은 경우
		if (n == t && t == th) {
			money = 10000 + n * 1000;
		} else if (n == t || n == th) {
			// 2개만 같은 경우
			money = 1000 + n * 100;
		} else if (t == th) {
			money = 1000 + t * 100;
		} else {
			// 모두 다른 경우
			int max = Math.max(n, Math.max(t, th));
			money = max * 100;
		}

		System.out.println(money);

		System.out.println();

		// Q120829
		int angle = 70;

		if (0 < angle && angle < 90) {
			System.out.println("예각 1");
		} else if (angle == 90) {
			System.out.println("직각 2");
		} else if (90 < angle && angle < 180) {
			System.out.println("둔각 3");
		} else {
			System.out.println("평각 4");
		}

		// Q120830
		int answer = 0;

		n = 10;
		int k = 3;
		int co = n / 10;

		n *= 12000;
		k *= 2000 - co;

		System.out.println(n + k);

	}

}
