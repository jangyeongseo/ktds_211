package exam;

public class ForExam2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 1. 1~5 답 : 1,2,3,4,5
		for (int i = 1; i < 6; i++) {
			System.out.println(i);
		}

		// 2. 5~1 답 : 5,4,3,2,1
		for (int i = 5; i > 0; i--) {
			System.out.println(i);
		}

	}

}
